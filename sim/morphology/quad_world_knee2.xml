<!-- << -->
<mujoco model="ant">
    <compiler angle="degree" coordinate="local" inertiafromgeom="true"/>
    <option integrator="RK4" timestep="0.01"/>

    <default>
        <joint armature="1" damping="10" limited="true"/>
        <geom conaffinity="0" condim="3" density="100.0" friction="2 0.5 0.5" margin="0.01" rgba="0.8 0.6 0.4 1"/>
        <tendon damping="300" stiffness="4000" />
    </default>

    <asset>
        <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
        <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
        <material name="geom" texture="texgeom" texuniform="true"/>
        <hfield file="heightfieldRough1.png" name="field1" size="8 8 0.05 0.15" />
    </asset>

    <worldbody>
        <!-- HeightField -->
        <geom condim="3" hfield="field1" name="highMapGeom1" pos="1 1 0" type="hfield" />
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
        <geom conaffinity="1" condim="3" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="40 40 40" type="plane"/>

        <body name="torso" pos="0 0 0.75">
            <geom name="torso_geom" pos="0 0 0" size="0.4 0.2 0.1" type="box"/>
            <joint armature="0" damping="0" limited="false" margin="0.01" name="root" pos="0 0 0" type="free"/>

            <body name="front_left_leg" pos="0.35 0.26 0.00">
                <joint axis="0 1 0" name="hip_1" pos="0.0 0.0 0.0" range="-45 45" type="hinge"/>
                <geom fromto="0.0 0.0 0.0 -0.2 0.0 -0.2" name="upper_1_geom" size="0.08" type="capsule"/>
                <site name="s1_1" pos="-0.2 0.0 -0.2" size="0.005" />
                <body name="aux_1" pos="-0.2 0.0 -0.2">
                    <joint axis="0 1 0" name="knee_1" pos="0 0 0" range="-45 45" type="hinge"/>
                    <geom fromto="0.0 0.0 0.0 0.2 0.0 -0.2" name="lower_1_geom" size="0.08" type="capsule"/>
                    <site name="s1_2" pos="0.2 0.0 -0.2" size="0.005" />
                </body>
            </body>

            <body name="front_right_leg" pos="0.35 -0.26 0.00">
                <joint axis="0 1 0" name="hip_2" pos="0.0 0.0 0.0" range="-45 45" type="hinge"/>
                <geom fromto="0.0 0.0 0.0 -0.2 0.0 -0.2" name="upper_2_geom" size="0.08" type="capsule"/>
                <site name="s2_1" pos="-0.2 0.0 -0.2" size="0.005" />
                <body name="aux_2" pos="-0.2 0 -0.2">
                    <joint axis="0 1 0" name="knee_2" pos="0 0 0" range="-45 45" type="hinge"/>
                    <geom fromto="0.0 0.0 0.0 0.2 0.0 -0.2" name="lower_2_geom" size="0.08" type="capsule"/>
                    <site name="s2_2" pos="0.2 0.0 -0.2" size="0.005" />
                </body>
            </body>

            <body name="left_back_leg" pos="-0.35 0.26 0.00">
                <joint axis="0 1 0" name="hip_3" pos="0.0 0.0 0.0" range="-45 45" type="hinge"/>
                <geom fromto="0.0 0.0 0.0 -0.2 0.0 -0.2" name="upper_3_geom" size="0.08" type="capsule"/>
                <site name="s3_1" pos="-0.2 0.0 -0.2" size="0.005" />
                <body name="aux_3" pos="-0.2 0 -0.2">
                    <joint axis="0 1 0" name="knee_3" pos="0 0 0" range="-45 45" type="hinge"/>
                    <geom fromto="0.0 0.0 0.0 0.2 0.0 -0.2" name="lower_3_geom" size="0.08" type="capsule"/>
                    <site name="s3_2" pos="0.2 0.0 -0.2" size="0.005" />
                </body>
            </body>

            <body name="right_back_leg" pos="-0.35 -0.26 0.00">
                <joint axis="0 1 0" name="hip_4" pos="0.0 0.0 0.0" range="-45 45" type="hinge"/>
                <geom fromto="0.0 0.0 0.0 -0.2 0.0 -0.2" name="upper_4_geom" size="0.08" type="capsule"/>
                <site name="s4_1" pos="-0.2 0.0 -0.2" size="0.005" />
                <body name="aux_4" pos="-0.2 0 -0.2">
                    <joint axis="0 1 0" name="knee_4" pos="0 0 0" range="-45 45" type="hinge"/>
                    <geom fromto="0.0 0.0 0.0 0.2 0.0 -0.2" name="lower_4_geom" size="0.08" type="capsule"/>
                    <site name="s4_2" pos="0.2 0.0 -0.2" size="0.005" />
                </body>
            </body>
        </body>
    </worldbody>

    <tendon>
        <spatial limited="true" name="tend1" range="0 1" rgba=".95 .3 .3 1" width="0.02">
            <site site="s1_1" />
            <site site="s1_2" />
        </spatial>
        <spatial limited="true" name="tend2" range="0 1" rgba=".95 .3 .3 1" width="0.02">
            <site site="s2_1" />
            <site site="s2_2" />
        </spatial>
        <spatial limited="true" name="tend3" range="0 1" rgba=".95 .3 .3 1" width="0.02">
            <site site="s3_1" />
            <site site="s3_2" />
        </spatial>
        <spatial limited="true" name="tend4" range="0 1" rgba=".95 .3 .3 1" width="0.02">
            <site site="s4_1" />
            <site site="s4_2" />
        </spatial>
    </tendon>

    <actuator>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="hip_1" gear="150"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="hip_2" gear="150"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="hip_3" gear="150"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="hip_4" gear="150"/>

        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="knee_1" gear="150"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="knee_2" gear="150"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="knee_3" gear="150"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="knee_4" gear="150"/>

        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="500" tendon="tend1" />
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="500" tendon="tend2" />
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="500" tendon="tend3" />
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="500" tendon="tend4" />

    </actuator>

</mujoco>

